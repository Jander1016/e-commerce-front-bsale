{
  "plugins": [
    "plugins/markdown"
  ],
  "source":{
    "include":["src"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
  "opts":{
    "recurse": true,
    "destination": "./doc",
    "template": "node_modules/docdash",
    "readme": "./README.md"
  }
} 